{
    "name": "spray-wrtc",
    "version": "0.0.1",
    "author": "Chat-Wane <grumpy.chat.wane@gmail.com>",
    "description": "Adaptive random peer sampling protocol running on top of WebRTC",
    "main": "./lib/membership.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/justayak/spray-wrtc.git"
    },
    "keywords": [
        "Random Peer Sampling",
        "Adaptive",
        "WebRTC"
    ],
    "dependencies": {
        "simple-peer": "5.9.x"
    },
    "bundledDependencies": [
        "simple-peer"
    ],
    "scripts":{
        "debug": "browserify -r ./lib/membership.js:spray-wrtc --debug > build/spray-wrtc.bundle.debug.js",
        "build": "browserify lib/membership.js --s spray-wrtc | uglifyjs > build/spray-wrtc.bundle.js"
    },
